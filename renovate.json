{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "helpers:pinGitHubActionDigests"
  ],
  "labels": [
    "kind/dependencies"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "major": {
    "enabled": true
  },
  "minor": {
    "enabled": true
  },
  "patch": {
    "enabled": true
  },
  "rebaseWhen": "behind-base-branch",
  "automerge": true,
  "prHourlyLimit": 10
}
